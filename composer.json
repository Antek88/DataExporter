{
    "name": "piotrantosik/dataexporter-bundle",
    "type": "symfony-bundle",
    "description": "Easy export data to CSV, XML, HTML, JSON or XLS",
    "keywords": ["exporter","csv","xls", "xml", "json"],
    "homepage": "http://github.com/piotrantosik/AntQaAjaxAutoCompleteBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Piotr Antosik",
            "email": "piotr@ant.qa",
            "homepage": "http://ant.qa",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.5.9",
        "symfony/framework-bundle": "~3.0",
        "symfony/http-foundation": "~3.0",
        "symfony/property-access": "~3.0",
        "symfony/options-resolver": "~3.0",
        "symfony/templating": "~3.0"
    },
    "require-dev": {
        "symfony/yaml": "~3.0",
        "knplabs/knp-snappy-bundle": "*"
    },
    "suggest": {
        "knplabs/knp-snappy-bundle": "To use PDF export",
        "symfony/templating": "To PDF and render export"
    },
    "autoload": {
        "psr-0": { "AntQa\\Bundle\\DataExporterBundle": "" }
    },
    "target-dir": "AntQa/Bundle/DataExporterBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "0.5.x-dev"
        }
    }
}